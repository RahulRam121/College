<web-app>

	<servlet>
		<servlet-name>servlet1</servlet-name>
		<servlet-class>CookieServlet1.FirstServ</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>servlet2</servlet-name>
		<servlet-class>CookieServlet1.SecondServ</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReadingCookiesExample</servlet-name>
		<servlet-class>CookieServlet2.ReadCookies</servlet-class>
	</servlet>

	<servlet-mapping>
